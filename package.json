{
  "name": "qunit-console-grouper",
  "version": "0.1.0",
  "description": "QUnit plugin that groups console messages by test",
  "keywords": [
    "console",
    "ember-addon",
    "qunit",
    "testing"
  ],
  "repository": "https://github.com/simplabs/qunit-console-grouper",
  "license": "MIT",
  "author": "simplabs GmbH",
  "main": "lib/qunit-console-grouper.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "changelog": "lerna-changelog",
    "lint": "eslint . --cache",
    "test": "jest"
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.2"
  },
  "devDependencies": {
    "ember-cli": "~3.12.0",
    "ember-cli-babel": "^7.11.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^3.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.2.1",
    "ember-source": "~3.13.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "lerna-changelog": "^0.8.2",
    "loader.js": "^4.7.0",
    "prettier": "1.18.2"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12.*"
  },
  "ember-addon": {
    "main": "index.js",
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-qunit",
      "ember-qunit"
    ]
  }
}
